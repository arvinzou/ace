.typeTag {
    width: 120rpx;
    height: 40rpx;
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    line-height: 40rpx;
    text-align: center;
    position: relative;
    margin-left: 40rpx;
}

.typeTag::before {
    content: '';
    display: block;
    position: absolute;
    border-width: 14rpx;
    border-style: solid;
    border-color: transparent;
    border-top-left-radius: 10rpx;
    transform: rotate(-45deg);
    left: -13rpx;
    top: 6rpx;
}

.typeTag::after {
    content: '';
    display: block;
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    background-color: #fff;
    border-radius: 50%;
    top: 15rpx;
    left: -10rpx;
}

