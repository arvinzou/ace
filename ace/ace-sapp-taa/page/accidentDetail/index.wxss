.index{
    width: 100%;
    height: 100%;
    background: #FAFAFA;
    margin-bottom: 71rpx;
}
.title{
    width: 90%;
    height: 100rpx;
    line-height: 100rpx;
    margin: 0 auto;
    color: #373F53;
    font-size: 34rpx;
    font-weight: bold;
}
.form-box{
    width: 90%;
    height: auto;
    margin: 0 auto;
}
.box{
    width: 100%;
    height: auto;
    background: #FFFFFF;
    border-radius: 10rpx;
}
.title02{
    width: 90%;
    margin: 0 auto;
    border-bottom: 1px solid #F0EFF6;
}
.locate{
    width: 100%;
    color: #373F53;
    font-size: 32rpx;
    font-weight: bold;
    padding-top: 48rpx;
    padding-bottom: 19rpx;
}
.locate-road{
    width: 100%;
    color: #ABAEB9;
    font-size: 28rpx;
    padding-bottom: 48rpx;
}
.locate-road image{
    width: 11rpx;
    height: 20rpx;
    margin-left: 22rpx;
}
.row{
    width: 90%;
    height: 110rpx;
    margin: 0 auto;
}
.row::after{
    content: '';
    clear: both;
    display: block;
}
.row-left{
    width: 25%;
    height: 110rpx;
    line-height: 110rpx;
    float: left;
    color: #373F53;
    font-size: 32rpx;
}
.row-right{
    width: 75%;
    height: 110rpx;
    float: left;
}
.content{
    line-height: 110rpx;
    color: #373F53;
    font-size: 32rpx;
    font-weight: bold;
    text-align: right;
}
.br{
    border-bottom:  1px solid #F0EFF6;
}
.content image{
    width: 11rpx;
    height: 20rpx;
    margin-left: 17rpx;
}
.row-right input{
    width: 100%;
    height: 100%;
    border: none;
    text-align: right;
    color: #373F53;
    font-size: 32rpx;
    font-weight: bold;
}
.input-holder{
    color: #ABAEB9;
    font-weight: 500;
}
.season-title{
    color: #373F53;
    font-size: 32rpx;
    padding-right: 20rpx;
}
.season-opt{
    font-size: 32rpx;
    color: #ABAEB9;
}
.row-title{
    line-height: 110rpx;
}
.cbox{
    width: 50%;
    height: auto;
    float: left;
    padding-top:30rpx;
    padding-bottom: 30rpx;
}
.checkbox-container::after{
    content: '';
    clear: both;
    display: block;
}
.commit-btn{
    width: 90%;
    height: 100rpx;
    line-height: 100rpx;
    color: #FFFFFF;
    font-size: 40rpx;
    font-weight: bold;
    text-align: center;
    margin: 77rpx 5%;
    background:rgba(67,80,252,1);
    box-shadow:0px 0px 10px 0px rgba(67, 252, 80, 0.35);
    border-radius: 50rpx;
}