    background:rgba(67,80,252,1);
    box-shadow:0rpx 0rpx 10rpx 0rpx rgba(67, 252, 80, 0.35);
    border-radius:50rpx;
    color: #FFFFFF;
    position: absolute;
    left: 10%;
    bottom: 60rpx;
}
.close-modal{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    bottom: 100rpx;
    left: 345rpx;
}
.secion-check{
    width: 28rpx;
    height: 28rpx;
    float: right;
}

