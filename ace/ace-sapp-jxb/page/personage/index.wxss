@import "../common/index.wxss";
@import "../common/lib/weui.wxss";
.iconList{
  width:16px; height: 16px;
  padding-right: 10rpx;
  vertical-align: middle;
}
.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.photo {
    max-height:70px;
    max-width:55px;
    vertical-align: middle;
}

.reg-top{
  background: #fff;
  flex-direction:column;
  padding: 20px;
}
.reg-title{
  font-size: 18px;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 10px;
}
.reg-icon{
  width:42px; 
  height: 42px; 
}
.reg-logo-panel{
    display: flex;
    align-items: center;
    justify-content: center;
}
.reg-logo-text{
  line-height: 42px;
  padding-left: 8px;
}
.navigator-box {
  background-color:#F8F8F8;
}
.navigator {
  background-color:#fff;
}
.bold{
  font-weight: 800
}

